@model string

<div class="m-content">
    <div class="m-portlet m-portlet--mobile">
        <div class="m-portlet__body">
            <form class="m-form m-form--label-align-right">
                <div class="row">
                    <div class="form-group col-lg-3 col-md-4 col-sm-6">
                        <h5>@Model</h5>
                    </div>
                </div>
                <div class="row">
                    <button type="button" class="btn btn-primary col-sm-6" onclick="window.close()">Close</button>
                </div>
            </form>
        </div>
    </div>
</div>
