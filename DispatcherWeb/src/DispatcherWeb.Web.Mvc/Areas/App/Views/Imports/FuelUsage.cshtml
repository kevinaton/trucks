@using System.Threading.Tasks
@using Abp.Configuration
@using Abp.Extensions
@using DispatcherWeb.Configuration
@using DispatcherWeb.Dto
@using DispatcherWeb.Features
@using DispatcherWeb.Infrastructure.Extensions
@using DispatcherWeb.Web.Areas.App.Startup
@using DispatcherWeb.PreventiveMaintenanceSchedule.Dto
@using DispatcherWeb.Web.Areas.App.Models.PreventiveMaintenanceSchedule
@{
    Layout = "../Imports/_ImportLayout";

    ViewBag.CurrentPageName = AppPageNames.Tenant.ImportFuelUsage;
    ViewBag.ImportType = ImportType.FuelUsage;
}

@section Scripts
{
    <environment names="Development">
        <script src="~/view-resources/Areas/app/Views/Imports/FuelUsage.js" asp-append-version="true"></script>
    </environment>
    <environment names="Staging,Production">
        <script src="~/view-resources/Areas/app/Views/Imports/FuelUsage.min.js" asp-append-version="true"></script>
    </environment>

}


@section H1
{
    Fuel usage import
}

@section AboveUpload
{
    <div class="row">
        <div class="form-group col-xs-12">
            <label for="FuelImportType">Fuel Import Type</label>
            <select id="FuelImportType" data-jacobus-value="jacobus" class="select2-preset">
                <option value="user">User Mapped</option>
                <option value="jacobus">Jacobus Energy</option>
            </select>
        </div>
    </div>
}
